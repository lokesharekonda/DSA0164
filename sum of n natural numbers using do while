#include <iostream>
using namespace std;

int main() {
    int n;
    cout << "Enter the value of n: ";
    cin >> n;

    int sum = 0;
    int number;
    int count = 0;

    cout << "Enter the numbers: ";
    do {
        cin >> number;
        sum += number;
        count++;
    } while (count < n);

    cout << "Sum: " << sum << endl;

    return 0;
}
