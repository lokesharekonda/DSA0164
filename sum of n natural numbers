#include <iostream>
using namespace std;

int main() {
    int n;
    cout << "Enter the value of n: ";
    cin >> n;

    int sum = 0;
    int number;

    cout << "Enter the numbers: ";
    for (int i = 0; i < n; ++i) {
        cin >> number;
        sum += number;
    }

    cout << "Sum: " << sum << endl;

    return 0;
}
