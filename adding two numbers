#include <iostream>
using namespace std;

int add(int a, int b) {
    return a + b;
}
int add(int a, int b, int c) {
    return a + b + c;
}
double add(double a, double b) {
    return a + b;
}
int main() {
    int intSum = add(5, 10);
    cout << "Sum of 5 and 10 is: " << intSum << endl;
    int intTripleSum = add(5, 10, 15);
    cout << "Sum of 5, 10 and 15 is: " << intTripleSum << endl;
    double doubleSum = add(5.5, 10.5);
    cout << "Sum of 5.5 and 10.5 is: " << doubleSum << endl;
    return 0;
}
